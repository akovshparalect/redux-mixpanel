{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
  },
  "rules": {
    "new-cap": 0,
    "no-use-before-define": 0,
    "object-shorthand": 0,
    "dot-notation": 0,
    "camelcase": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": 0,
    "no-multi-spaces": 0,
    "no-param-reassign": [2, {"props": true}],
    "comma-dangle": [0, "always"],
    "semi": [2, "never"],
    "object-curly-spacing": [0, "always"],
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-else-return": 0,
    "no-console": 2,
    "no-throw-literal": 0,
    "id-length": 0,
    "no-unused-expressions": 2,
    "space-before-function-paren": 0
  }
}
